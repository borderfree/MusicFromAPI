<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="4dp">
        <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:id="@+id/TextView01" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/fusioncharts_editor_psa_ng_ml"/>
        <EditText android:id="@+id/fusionchartEditorEditTextPSA" android:layout_width="0dp" android:layout_height="wrap_content" android:hint="0" android:ems="10" android:layout_weight="1" android:inputType="numberDecimal"/>
    </LinearLayout>
    <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="4dp">
        <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:id="@+id/fusionchartsEditorTextViewRatio" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/fusioncharts_editor_free_total_psa_ratio"/>
        <SeekBar android:id="@+id/fusionchartEditorSeekBarFreeTotalRatio" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="4dp"/>
    </LinearLayout>
    <LinearLayout android:orientation="horizontal" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="4dp">
        <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:gravity="center_vertical" android:id="@+id/TextView02" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/fusioncharts_editor_date" android:layout_weight="1"/>
        <Button android:id="@+id/fusionchartEditroPickDateButton" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="" android:layout_weight="0"/>
    </LinearLayout>
    <Button android:id="@+id/fusionchartEditorButtonAddItem" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_margin="4dp" android:text="@string/add"/>
    <ListView android:id="@+id/fusionchartEditorItemsList" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_margin="4dp"/>
</LinearLayout>
